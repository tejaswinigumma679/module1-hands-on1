<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StyledButton">
        <attr name="buttonType" format="enum">
            <enum name="primary" value="0"/>
            <enum name="secondary" value="1"/>
            <enum name="danger" value="2"/>
            <enum name="success" value="3"/>
            <enum name="warning" value="4"/>
        </attr>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="isLoading" format="boolean"/>
    </declare-styleable>
</resources>